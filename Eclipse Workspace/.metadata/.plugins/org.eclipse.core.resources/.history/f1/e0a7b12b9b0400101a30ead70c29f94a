package com.android.hockeyapp;

import android.text.format.Time;

public class Game {
	//What does a game have
	//date
	//time
	//location
	//opponent
	Time t;
	String location;
	String opponent;
	boolean home;
	
	public Game(Time ti, String l, String o, boolean h){
		t = ti;
		location = l;
		opponent = o;
		atHome = h;
	}
	
}
