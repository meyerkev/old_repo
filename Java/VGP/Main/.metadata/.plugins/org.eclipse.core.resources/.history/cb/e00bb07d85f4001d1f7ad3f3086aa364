//version 1
public class HelloWorld {
	public static void main(String[] args) {
		System.out.println("Hello World");
		String b = "hey\nhey";
		System.out.println(b);	
		char a = 'a';
		System.out.println(a);
		b = "Hey!\nNot!";
		System.out.print(b);	
		a = 'b';
		System.out.println(a);
	}

}
